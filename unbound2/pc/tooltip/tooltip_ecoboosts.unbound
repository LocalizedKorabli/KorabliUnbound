(def element EconomicBoostSlotTooltipTTXNoBoostInstalled (_boostTypeModifier:str)
	(scope
		(var imageUrl:str = "'url:../eco_boosts/factors/' + _boostTypeModifier + '.png'")
		(var commonBoostTooltipHeaderText:str = "tr('IDS_ECONOMIC_BOOST_TYPE_' + toUpper(_boostTypeModifier) + '_SIMPLE_SUMMARY_TOOLTIP') + tr('IDS_TOOLTIP_SPACE')")
	)
	(style (hitTest = false) (width = "DEFAULT_TOOLTIP_WIDTH"))

	(macro TOOLTIP_SYSTEM_DEFAULT_BACKGROUND)
	(element TOOLTIP_SYSTEM_DEFAULT_CONTAINER
		(element TooltipSystemHeaderWithIconAndText
			_imageUrl =			"imageUrl"
			_imageWidth =		80
			_imageHeight =		80
			_unifiedStatus =	"SC.Ui_styles.UNIFIED_STATUS.DEFAULT"
			_headerText =		"commonBoostTooltipHeaderText"
			_subheaderText =	'IDS_ECONOMIC_BOOST_TYPE'
		)

		(element TooltipSystemHorizontalDivider)
		(element TooltipSystemStatusLine
			_text =				'IDS_ECONOMIC_BOOSTS_TTX_NOT_INSTALLED_HINT'
			_unifiedStatus =	"SC.Ui_styles.UNIFIED_STATUS.ATTENTION"
		)

		(element TooltipSystemHorizontalDivider)
		(element TooltipSystemStatusLine
			_text =				'IDS_ECONOMIC_BOOSTS_TTX_GO_TO_MODAL_WINDOW_TOOLTIP'
			_unifiedStatus =	"SC.Ui_styles.UNIFIED_STATUS.MOUSE_LEFT"
		)
	)
)